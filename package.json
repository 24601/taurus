{
  "name": "@bullhorn/taurus",
  "version": "0.0.0",
  "description": "A simple, restful, api layer for Bullhorn",
  "keywords": [
    "ta",
    "bullhorn"
  ],
  "homepage": "http://bullhorn.github.io/symphony",
  "bugs": {
    "url": "https://github.com/bullhorn/symphony/issues"
  },
  "license": "MIT",
  "author": "Brian Kimball <bkimball@bullhorn.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/bullhorn/symphony"
  },
  "publishConfig": {
    "registry": "http://hydrogen.bh-bos2.bullhorn.com/artifactory/api/npm/npm-dev"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">= 6.2.1",
    "npm": ">= 5"
  },
  "dependencies": {
    "@bullhorn/bullhorn-types": "^0.0.2",
    "axios": "^0.17.1",
    "rxjs": "^5.5.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "@types/node": "^8.0.0",
    "jest": "^22.0.0",
    "opn-cli": "^3.1.0",
    "rollup": "^0.54.0",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.0",
    "rollup-plugin-typescript2": "^0.10.0",
    "rollup-plugin-uglify": "^2.0.0",
    "sinon": "^4.1.0",
    "temp-dir": "^1.0.0",
    "ts-jest": "^22.0.0",
    "ts-node": "~4.1.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.0",
    "typescript": "~2.6.x"
  },
  "scripts": {
    "build": "rollup -c",
    "build:es2015": "tsc --module es2015 --outDir lib/es2015 --declaration --declarationDir lib/typings",
    "build:rollup": "rollup -c",
    "clean": "rm -rf lib",
    "lint": "tslint --project tsconfig.json --format stylish",
    "test": "jest --config jest.config.json --coverage --no-cache --runInBand",
    "test:watch": "jest --config jest.config.json --watch",
    "posttest": "npm run lint",
    "preversion": "npm run build && npm test",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "publish:docs": "easystatic deploy docs --repo bullhorn/symphony",
    "docs": "typedoc src/index.ts --excludePrivate --theme node_modules/typedoc-clarity-theme/bin --mode file --out docs && opn docs/index.html",
    "docs:json": "typedoc --mode file --json build/docs/typedoc.json src/index.ts"
  }
}
